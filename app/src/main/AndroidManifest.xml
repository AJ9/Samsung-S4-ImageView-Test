            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PicassoActivity"
            android:label="@string/title_activity_picasso" >
        </activity>
        <activity
            android:name=".BitmapActivity"
            android:label="@string/title_activity_bitmap" >
        </activity>
        <activity
        android:name=".CorrectSolutionActivity"
        android:label="Correct Solution" >
    </activity>
    </application>

</manifest>
